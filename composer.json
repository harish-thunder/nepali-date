{
    "name": "makka/nepali_date",
    "description": "A simple plugin to convert nepali date to english date",
    "keywords": [
        "makka",
        "package",
        "laravel",
        "nepali_date"
    ],
    "homepage": "https://github.com/php-loep/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":makka",
            "email": "mangitma@gmail.com",
            "homepage": "http://mangitmaharjan.com.np",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "nesbot/carbon": "^2.29.1"
    },
    "autoload": {
        "psr-4": {
            "Makka\\NepaliDate\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "aliases": {
                "NDate": "Makka\\NepaliDate\\NepaliDate"
            }
        }
        
    }
}
